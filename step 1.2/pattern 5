public class Solution {
    public static void seeding(int n) {
        for(int row = 0; row < n; row++)
        {
            for(int col = 0; col < n; col++)
            {
                if(col < (n-row))
                {
                    System.out.print("* ");
                }
            }
            System.out.println();
        }
    }
}