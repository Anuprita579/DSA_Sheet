import java.util.HashMap;
public class Solution {
    public static int[] countFrequency(int n, int x, int []num){
        int [] a = new int [n];
        for (int i=0; i<n; i++){
            if(num[i]>0 && num[i]<=n){
                a[num[i]-1] += 1;
            }
        }
        return a;
    }
}